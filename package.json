{
  "name": "sanity-plugin-vimeo-sync",
  "version": "2.3.0",
  "description": "Sync Vimeo PRO videos into Sanity",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/tristantbg/sanity-plugin-vimeo-sync#readme",
  "bugs": {
    "url": "https://github.com/tristantbg/sanity-plugin-vimeo-sync/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/tristantbg/sanity-plugin-vimeo-sync.git"
  },
  "license": "MIT",
  "author": "Tristan Bagot <hello@tristanbagot.com>",
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "plugin-kit verify-package --silent && pkg-utils build --strict --check --clean",
    "dev": "pnpm link-watch",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "watch": "pkg-utils watch --strict"
  },
  "browserslist": "extends @sanity/browserslist-config",
  "dependencies": {
    "@sanity/color": "^3.0.6",
    "@sanity/icons": "^3.7.4",
    "@sanity/incompatible-plugin": "^1.0.5",
    "@sanity/studio-secrets": "^4.0.1",
    "@sanity/ui": "^3.1.11",
    "hls-video-element": "^1.5.10",
    "media-chrome": "^4.17.2",
    "nanoid": "^5.1.6",
    "react-icons": "^5.5.0",
    "yalc": "1.0.0-pre.53"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "^10.4.4",
    "@sanity/plugin-kit": "^4.0.20",
    "@types/react": "^19.2.14",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-sanity": "^7.1.4",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "prettier": "^3.8.1",
    "prettier-plugin-packagejson": "^3.0.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "sanity": "^5.10.0",
    "styled-components": "6.3.9",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": "^18 || ^19",
    "sanity": "^3 || ^5"
  },
  "packageManager": "pnpm@10.28.1+sha512.7d7dbbca9e99447b7c3bf7a73286afaaf6be99251eb9498baefa7d406892f67b879adb3a1d7e687fc4ccc1a388c7175fbaae567a26ab44d1067b54fcb0d6a316",
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "scrollmirror",
      "yarn"
    ]
  },
  "sanityPlugin": {
    "verifyPackage": {
      "eslintImports": false
    }
  }
}
