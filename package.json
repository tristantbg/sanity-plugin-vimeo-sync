{
  "name": "sanity-plugin-vimeo-sync",
  "version": "1.3.0",
  "description": "Sync Vimeo PRO videos into Sanity",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/tristantbg/sanity-plugin-vimeo-sync#readme",
  "bugs": {
    "url": "https://github.com/tristantbg/sanity-plugin-vimeo-sync/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/tristantbg/sanity-plugin-vimeo-sync.git"
  },
  "license": "MIT",
  "author": "Tristan Bagot <hello@tristanbagot.com>",
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "plugin-kit verify-package --silent && pkg-utils build --strict --check --clean",
    "dev": "pnpm link-watch",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "watch": "pkg-utils watch --strict"
  },
  "dependencies": {
    "@sanity/color": "^3.0.6",
    "@sanity/icons": "^3.7.0",
    "@sanity/incompatible-plugin": "^1.0.5",
    "@sanity/studio-secrets": "^3.0.1",
    "@sanity/ui": "^2.15.7",
    "nanoid": "^5.1.4",
    "react-icons": "^5.5.0",
    "yalc": "1.0.0-pre.53"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "^7.1.1",
    "@sanity/plugin-kit": "^4.0.19",
    "@types/react": "^19.0.11",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-config-sanity": "^7.1.4",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.5.3",
    "prettier-plugin-packagejson": "^2.5.10",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sanity": "^3.80.0",
    "styled-components": "6.1.16",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "^3"
  },
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "scrollmirror",
      "yarn"
    ]
  },
  "sanityPlugin": {
    "verifyPackage": {
      "eslintImports": false
    }
  }
}
